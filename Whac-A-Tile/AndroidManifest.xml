        <activity
            android:name="com.whac_a_tile.VideoPage"
            android:label="@string/title_activity_video_page" >
        </activity>
        <activity
            android:name="com.whac_a_tile.GameActivity"
            android:label="@string/title_activity_game" >
        </activity>
        <activity
            android:name="com.whac_a_tile.AddScore"
            android:label="@string/title_activity_add_score" >
